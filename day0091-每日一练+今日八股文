}

//学生基本信息的输入输出
https://www.nowcoder.com/practice/58b6a69b4bf943b49d2cd3c15770b9fd?tpId=107&&tqId=33292&rp=1&ru=/ta/beginner-programmers&qru=/ta/beginner-programmers/question-ranking
int main() {
    int a;
    float b,c,d;
    scanf("%d;%f,%f,%f",&a,&b,&c,&d);
    printf("The each subject score of No. %d is %.2lf, %.2lf, %.2lf.",a,b,c,d);
    return 0;
}

//判断是否是大小写字母
https://www.nowcoder.com/practice/44d2d63103664913bc243d3836b4f341?tpId=107&&tqId=33316&rp=1&ru=/ta/beginner-programmers&qru=/ta/beginner-programmers/question-ranking
int main() {
    char a;
    scanf("%c",&a);
    if ((a>='a' && a<='z')||(a>='A'&&a<='Z') )
    printf("YES");
    else
    printf("NO");
    return 0;
}
